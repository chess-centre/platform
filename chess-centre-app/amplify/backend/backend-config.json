{
  "auth": {
    "platformchesscentreapp": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "platformchesscentreappPostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "platformchesscentreappPreTokenGeneration",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  },
  "api": {
    "platformchesscentreapp": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authplatformchesscentreapp"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "API_KEY",
              "apiKeyConfig": {
                "apiKeyExpirationDays": 30,
                "description": "api key description"
              }
            },
            {
              "authenticationType": "AWS_IAM"
            }
          ]
        }
      }
    },
    "public": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "stripeCheckout",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "stripeWebhook",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "stripeCustomerPortal",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "eventRegister",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "chessplayers": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "ecfPlayer",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "ecfPlayerGames",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "stripeCheckout": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "platformchesscentreapp",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "stripeWebhook": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "platformchesscentreapp",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ]
        }
      ]
    },
    "platformchesscentreappPostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "platformchesscentreappPreTokenGeneration": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "stripeCustomerPortal": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "platformchesscentreapp",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    },
    "eventRegister": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "platformchesscentreapp",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ]
        }
      ]
    },
    "ecfPlayer": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "ecfPlayerGames": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "ecfRatingScrapper": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "getMemberGames": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "platformchesscentreapp",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    }
  },
  "storage": {
    "pgn": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  },
  "analytics": {
    "platformchesscentreaKinesis": {
      "service": "Kinesis",
      "providerPlugin": "awscloudformation"
    }
  }
}