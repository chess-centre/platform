{
  "version": 1,
  "paths": {
    "/user/{id}": {
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ]
      },
      "lambdaFunction": "getLichessPlayer"
    },
    "/statuses": {
      "name": "/statuses",
      "lambdaFunction": "getLichessStatuses",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    }
  }
}